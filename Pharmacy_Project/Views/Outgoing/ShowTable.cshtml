@{
    ViewData["Title"] = "Исходящий";
    @model List<Outgoing>;
}

<div class="text-center">
     <table class="table">
        <tr>
            <th>Название лекарства</th>
            <th>Дата поставки</th>
            <th>Количество</th>
            <th>Цена</th>
        </tr>
        @foreach(var outgoing in Model) {
            <tr>
                <td>
                    @outgoing.Medicine.Name
                </td>
                <td>
                    @outgoing.ImplementationDate
                </td>
                <td>
                    @outgoing.Count
                </td>
                <td>
                    @outgoing.SellingPrice
                </td>
            </tr>
        }
    </table>
</div>
